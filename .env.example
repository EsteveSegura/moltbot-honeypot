# Service to mimic: moltbot, clawdbot, openclaw
HONEYPOT_SERVICE_NAME=clawdbot

# Honeypot server
HONEYPOT_HOST=0.0.0.0
HONEYPOT_PORT=18789

# Admin dashboard (honeypot monitoring)
ADMIN_HOST=127.0.0.1
ADMIN_PORT=41892
ADMIN_USERNAME=admin
ADMIN_PASSWORD=change-this-password

# Public UI (mimics real ClawdBot UI)
UI_ENABLED=true
UI_HOST=0.0.0.0
UI_PORT=80

# mDNS/Bonjour (disabled by default, use avahi-daemon instead)
MDNS_ENABLED=false
MDNS_HOSTNAME=workstation

# Data storage
DATA_DIR=./data
MAX_ATTACKS_MEMORY=10000
